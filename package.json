{
  "name": "with-vercel-website",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "node scripts/ensure-next-manifests.cjs && cross-env NODE_OPTIONS=--no-deprecation next dev -p 3000",
    "dev:neon": "node scripts/ensure-next-manifests.cjs && cross-env USE_NEON=true NODE_OPTIONS=\"--no-deprecation -r dotenv/config\" next dev -p 3000",
    "dev:lan": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3000 --hostname 0.0.0.0",
    "dev:free-port": "lsof -ti :3000 | xargs kill -9 2>/dev/null; sleep 1; node scripts/ensure-next-manifests.cjs && pnpm run dev",
    "dev:clean": "rm -rf .next && pnpm run dev:free-port",
    "dev:reset": "lsof -ti :3000 | xargs kill -9 2>/dev/null; rm -rf .next; echo 'Stopped :3000 and removed .next. Run: pnpm run dev'",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "db:push": "cross-env PAYLOAD_FORCE_DRIZZLE_PUSH=true NODE_OPTIONS=--no-deprecation next dev -p 3000",
    "db:reset": "cross-env PAYLOAD_DROP_DATABASE=true NODE_OPTIONS=--no-deprecation next dev -p 3000",
    "fix:sqlite-schema": "tsx src/scripts/fix-sqlite-schema.ts",
    "seed:globals": "cross-env PAYLOAD_MIGRATING=true tsx src/scripts/seed-globals.ts",
    "unlock-users": "tsx src/scripts/unlock-users.ts",
    "reset-password": "cross-env PAYLOAD_MIGRATING=true tsx src/scripts/reset-password.ts",
    "reset-password-direct": "tsx src/scripts/reset-password-direct.ts",
    "export:local": "cross-env DATABASE_URL= POSTGRES_URL= NODE_ENV=production tsx src/scripts/export-data.ts",
    "import:neon": "tsx src/scripts/import-data.ts --replace",
    "publish-pages": "tsx src/scripts/publish-site-pages.ts",
    "check-pages": "tsx src/scripts/check-site-pages-api.ts",
    "migrate:neon": "cross-env USE_NEON=true NODE_OPTIONS=\"--no-deprecation -r dotenv/config\" payload migrate",
    "inspect:neon": "tsx src/scripts/inspect-neon-schema.ts",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --import=tsx/esm\" playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "ci": "payload migrate && next build",
    "postinstall": "node scripts/link-richtext-lexical-uuid.cjs"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@fontsource/outfit": "^5.2.8",
    "@payloadcms/admin-bar": "3.76.1",
    "@payloadcms/db-postgres": "3.76.1",
    "@payloadcms/db-sqlite": "3.76.1",
    "@payloadcms/db-vercel-postgres": "3.76.1",
    "@payloadcms/live-preview-react": "3.76.1",
    "@payloadcms/next": "3.76.1",
    "@payloadcms/plugin-form-builder": "3.76.1",
    "@payloadcms/plugin-nested-docs": "3.76.1",
    "@payloadcms/plugin-redirects": "3.76.1",
    "@payloadcms/plugin-search": "3.76.1",
    "@payloadcms/plugin-seo": "3.76.1",
    "@payloadcms/richtext-lexical": "3.76.1",
    "@payloadcms/storage-vercel-blob": "3.76.1",
    "@payloadcms/ui": "3.76.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "graphql": "^16.8.2",
    "libsql": "^0.4.7",
    "lucide-react": "0.563.0",
    "motion": "^12.34.2",
    "next": "15.4.11",
    "next-sitemap": "^4.2.3",
    "payload": "3.76.1",
    "prism-react-renderer": "^2.3.1",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "7.71.1",
    "sharp": "0.34.2",
    "tailwind-merge": "^3.4.0",
    "three": "^0.183.0",
    "vanta": "^0.5.24",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.2",
    "@types/node": "22.19.9",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@types/three": "^0.183.0",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.11",
    "jsdom": "28.0.0",
    "mini-css-extract-plugin": "^2.10.0",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.18",
    "tsx": "4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "6.0.5",
    "vitest": "4.0.18"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.29.3"
}
